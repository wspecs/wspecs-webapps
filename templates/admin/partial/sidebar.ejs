<div class="sidebar">
  <nav class="sidebar-nav">
    <ul class="nav">
      <% for (const item of adminSideMenu) { %>
        <% if (item.subMenu) { %>
         <li class="nav-item nav-dropdown">
           <a class="nav-link nav-dropdown-toggle" href="#"><i class="icon-<%- item.icon %>"></i> <%- item.title %></a>
           <ul class="nav-dropdown-items">
             <% for (const sub of item.subMenu) { %>
               <li class="nav-item">
                 <a class="nav-link" href="<%- base %><%- sub.href %>">
                   <i class="icon-<%- sub.icon || item.icon %>"></i> <%- sub.title %>
                 </a>
               </li>
             <% } %>
           </ul>
         </li>

        <% } else { %>
         <li class="nav-item">
           <a class="nav-link" href="<%- base %><%- item.href %>">
             <i class="icon-<%- item.icon %>"></i> <%- item.title %>
             <% if (item.badge) { %>
               <span class="badge badge-info"><%- item.badge %></span>
             <% } %>
           </a>
         </li>
        <% } %>
      <% } %>
     </ul>
  </nav>
</div>


